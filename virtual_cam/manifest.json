{"manifest_version":3
,"name":"Slava Ukraini Virtual Webcam"
,"version":"0.1.1"
,"description":"Virtual Webcam with Flag of Ukraine"
,"icons":{"16":"icon-16.png"
  ,"32":"icon-32.png"
  ,"48":"icon-48.png"
  ,"128":"icon-128.png"
  }
,"minimum_chrome_version":"10.0"
,"options_ui":{"page":"js/options.html"}
,"content_scripts":[{"matches":["<all_urls>"]
   ,"js":["js/inject.js"]
   ,"run_at":"document_start"
   ,"all_frames":true
}]
,"permissions":["tabs","storage"]
,"web_accessible_resources":[{"resources":["js/*"]
   ,"matches":["<all_urls>"]
}]
}